{
  "name": "@evac/fonts",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "fonts",
    "Fira Mono",
    "Montserrat"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "default": "./dist/vite.js"
    },
    "./fira-mono/*": "./fira-mono/*",
    "./montserrat/*": "./montserrat/*"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "./vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "scripts": {
    "postinstall": "vite build",
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.vue"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "peerDependencies": {
    "vite": "^7.1.7"
  }
}
