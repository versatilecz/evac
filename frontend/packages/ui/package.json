{
  "name": "@evac/ui",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./theme.css": "./src/theme.css"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "postinstall": "vite build",
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.vue"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@evac/shared": "*",
    "@evac/utils": "*",
    "remeda": "^2.32.0",
    "reka-ui": "^2.5.1",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3"
  }
}
